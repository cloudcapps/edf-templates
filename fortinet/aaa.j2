config user radius
    edit "EDFR ISE"
        set server "{{server.['SDHQ-ISE-01']}}"
        set secret {{radius.key}}
        set source-ip "{{device.interface['MANAGEMENT'].ip}}}"
        set interface-select-method sdwan
        set secondary-server "server.['COLO-ISE-01']"
        set secondary-secret {{radius.key}}
    next
end